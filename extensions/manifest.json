{
  "manifest_version": 2,
  "name": "pbkit-devtools",
  "version": "0.1",
  "description": "Devtool for pbkit",
  "icons": {
      "128": "icons/pbkit-128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["main.js"]
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "devtools_page": "devtools.html"
}

